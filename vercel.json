{
  "name": "my-nodejs-project",
  "builds": [
    {
      "src": "server.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token" 
  },
  "github": {
    "silent": true,
    "enable": true
  }
}
